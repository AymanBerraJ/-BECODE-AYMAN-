<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('partials/head.ejs', { title: title }) %>
  </head>
  <body>
    <% if(users.length) { %>
      <ul>
        <% users.forEach(user => { %>
          <li>
            <p>Prénom: <%= user.firstName %></p>
            <p>Nom: <%= user.lastName %></p>
            <p>Âge: <%= user.age %></p>
          </li>
        <% }) %>
      </ul>
    <% } else { %>
      <h1>Aucun membre enregistré</h1>
    <% } %>
    <a href="/add">Register</a>
  </body>
</html>
